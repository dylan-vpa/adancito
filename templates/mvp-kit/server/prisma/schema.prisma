generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

// Example model - will be replaced by generated code
model User {
  id        String   @id @default(uuid())
  email     String   @unique
  name      String?
